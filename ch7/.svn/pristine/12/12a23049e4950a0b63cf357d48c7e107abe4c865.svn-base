    font-size: 20px;
    margin-top: 5px;
    font-weight: 100;
}


/*5.====================== 电子书 ====================== */

.ebooks {
    height: 496px;
    background-color: #fff;
    margin-top: 34px;
}


/* 左侧盒子 */

.ebooks .left-box {
    width: 906px;
    height: 496px;
}


/* 顶部 */

.ebooks .left-box .top {
    width: 906px;
    height: 40px;
    line-height: 41px;
