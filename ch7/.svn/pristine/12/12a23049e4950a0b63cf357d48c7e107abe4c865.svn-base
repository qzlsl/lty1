/*12.====================== 二维码  ====================== */

.qr-code {
    width: 40px;
    height: 180px;
    background-color: red;
    /* 定位 */
    position: fixed;
    top: 185px;
    right: 0;
}

.qr-code p {
    width: 40px;
    height: 40px;
    background-color: pink;
}

.qr-code p img {
    width: 100%;
    height: 100%;
}

.qr-code p:nth-child(2) {
    height: 100px;
    background-color: plum;
}

.qr-code div {
    /* 定位 */
    position: absolute;
    top: 40px;
    left: 0;
    z-index: -10;
}


/*12.====================== 顶部搜索框交互 ====================== */

.top-search-box {
    width: 100%;
    height: 40px;
    line-height: 40px;
    background-color: rgba(243, 95, 10, 0.75);
    /* 定位 */
    position: fixed;
    top: 0;
    left: 0;
    /* 默认隐藏 */
    display: none;
}

.top-search-box .search-box {
    width: 400px;
    height: 30px;
    line-height: 30px;
    background-color: #fff;
    margin: 6px auto;
    background-image: url('../images/img/bgicon.png');
    background-repeat: no-repeat;
    background-position: 357px -2px;
}

.top-search-box .search-box input {
    height: 26px;
    width: 360px;
    border: 2px solid #ff6700;
    outline: none;
    text-indent: 10px;
}


/*12.====================== 楼层滚动样式 ======================= */

.floor {
    width: 40px;
    height: 200px;
    background-color: #f2f2f2;
    position: fixed;
    margin-left: 50%;
    left: -640px;
    top: 200px;
}

.floor li {
    width: 40px;
    height: 40px;
    overflow: hidden;
    /* 设置过渡效果 */
    transition: all 0.3s;
}

.floor li:hover {
    width: 80px;
    color: #fff;
    background-color: #93d56f;
}

.floor li img {
    width: 40px;
    height: 40px;
}