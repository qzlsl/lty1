/*1.====================== 顶部 ====================== */


/*顶部外层容器*/

.topbar {
    height: 28px;
    background-color: rgb(0, 0, 0);
}


/*顶部里层容器*/

.topbar .container {
    height: 28px;
    background-color: rgb(0, 0, 0);
}


/*里面的P标签*/

.topbar .container p {
    height: 28px;
    line-height: 28px;
    color: rgb(255, 255, 255);
}


/*第一个p标签*/

.topbar .container p:first-child {
    text-indent: 210px;
}


/*第一个p标签下面的a标签*/

.topbar .container p:first-child a {
    color: seashell;
    text-decoration: none;
}


/*第二个p标签*/

.topbar .container p:first-child(2) {
    margin-right: 10px;
}


/*第二个p标签 下面的标题 和 注册*/

.topbar .container p:nth-child(2) a:not(.login) {
    color: #fd7d27;
}


/*第二个p标签下面的登录*/

.topbar .container p:nth-child(2) .login {
    color: snow;
}


/*2.====================== 导航 ====================== */

.nav {
    height: 84px;
    background-color: #cccccc;
}


/*搜索购物车订单大容器*/

.nav .search-shopcart-order {
    margin-top: 5px;
    height: 35px;
}


/*搜索框*/

.nav .search-shopcart-order .search-input {
    width: 440px;
    height: 45px;
    background-color: 口#ff6700;
    margin-right: 38px;
    position: relative;
}


/*搜索框中的图标span*/

.nav .search-shopcart-order .search-input span {
    display: inline;
    width: 50px;
    height: 30px;
    position: absolute;
    right: 0;
    top: 0;
    margin-top: 8px;
    background-image: url('../images/img/bgicon.png');
}


/*搜索框*/

.nav .search-shopcart-order .search-input .search {
    width: 390px;
    height: 26px;
    border: 2px solid #ff6700;
    text-indent: 10px;
    color: #333;
    outline: none;
    margin-top: 8px;
}


/*购物车和订单*/

.nav .search-shopcart-order .shopcart-order {
    width: 200px;
    height: 35px;
    font-size: 0em;
}


/*购物车订单按钮*/

.nav .search-shopcart-order .shopcart-order button {
    width: 100px;
    height: 35px;
    background-color: #ebebeb;
    border: 1px solid #ffffff;
    font-size: 13px;
    color: #333;
}


/*购物车图标*/

.nav .search-shopcart-order .shopcart-order button:nth-child(1) {
    background-color: #ff6700;
    color: #fff;
    background-image: url(../images/img/bgicon.png);
    background-repeat: no-repeat;
    background-position: -4px -44px;
    margin-top: 8px;
}


/*分类导航*/

.nav .category-menu {
    margin-top: 15px;
    height: 30px;
    background-color: red;
}

.nav .category-menu .left-logo {
    width: 200px;
    height: 30px;
    background-color: aquamarine;
}

.nav .category-menu .left-logo img {
    position: absolute;
    top: 4px;
    left: 161;
}

.nav .category-menu ul {
    width: 1000px;
    height: 33px;
    background-color: rgb(250, 250, 250);
}

.nav .category-menu ul li {
    float: left;
    height: 30px;
    line-height: 30px;
    width: 80px;
    color: #333;
    font-size: 14px;
    text-align: center;
    font-weight: 700;
}


/*3.====================== 分类导航和banner====================== */

.menu {
    height: 430px;
}


/* 左侧导航 */

.menu .sideMenu {
    height: 430px;
    width: 200px;
}


/* 导航列表 */

.menu .sideMenu ul {
    height: 430px;
    width: 200px;
    background-color: #f3f3f3;
    position: relative;
}


/* 导航li */

.menu .sideMenu ul li {
    height: 30px;
    line-height: 30px;
    width: 200px;
    text-indent: 20px;
}


/* 导航下的a标签 */

.menu .sideMenu ul li a {
    color: #353535;
    text-decoration: none;
}


/* 导航li激活效果 */

.menu .sideMenu ul li:hover {
    background-color: #fff;
}


/* li激活下a标签的样式 */

.menu .sideMenu ul li:hover a {
    color: #ff6700;
}


/* 一级导航标题 */

.menu .sideMenu ul li .title {
    width: 200px;
    height: 30px;
    line-height: 30px;
}


/* li激活下一级导航标题 */

.menu .sideMenu ul li:hover .title {
    background-color: #fff;
    position: absolute;
    z-index: 99;
}


/* 激活显示二级导航 */

.menu .sideMenu ul li:hover .subMenu {
    display: block;
}


/* 二级导航效果 */

.menu .sideMenu ul li .subMenu {
    position: absolute;
    left: 198px;
    top: 0;
    background-color: #fff;
    border: 1px solid #ff6700;
    width: 500px;
    height: 428px;
    display: none;
    z-index: 88;
}


/* 轮播容器 */

.menu .banner {
    height: 430px;
    width: 1000px;
}


/*4.====================== 今日推荐 ====================== */

.recommend {
    height: 544px;
}

.recommend .title {
    widows: 1200px;
    height: 40px;
    line-height: 40px;
    margin-top: 10px;
    text-indent: 24px;
    font-size: 24px;
    font-weight: 300px;
    color: #ff6700;
    background-color: #fff;
    border-bottom: 6px solid #ff6700;
}


/* 推荐内容 */

